{
  "version": "0.2.0",
  "configurations": [
  {
    "name": "Launch test.js",
    "type": "node",
    "request": "launch",
    "cwd": "${workspaceFolder}",
    "runtimeExecutable": "node",
    "args": [ "./build/test.js" ],
    "outputCapture": "std"
  },
    {
      "name": "Debug Main Process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "args" : ["./build/main.js"],
      "outputCapture": "std"
    }
  ]
}

